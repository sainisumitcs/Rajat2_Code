package height;

class node {

	public int data;
	public node left;
	
	public node right;
	

	public node getPre() {
		return right;
	}

	public void setPre(node pre) {
		this.right = pre;
	}

	public  int getData() {
		return data;
	}

	public  void setData(int data) {
		this.data = data;
	}

	public  node getNext() {
		return next;
	}

	public  void setNext(node next) {
		this.next = next;
	}

}
